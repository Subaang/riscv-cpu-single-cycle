#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a70eadbb4f0 .scope module, "tb_top" "tb_top" 2 3;
 .timescale -9 -12;
v0x5a70eae03f60_0 .var "clk", 0 0;
v0x5a70eae04000_0 .var "reset", 0 0;
S_0x5a70eadbb680 .scope module, "uut" "top" 2 10, 3 1 0, S_0x5a70eadbb4f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
v0x5a70eadbb860_0 .net "clk", 0 0, v0x5a70eae03f60_0;  1 drivers
v0x5a70eae03e40_0 .net "reset", 0 0, v0x5a70eae04000_0;  1 drivers
    .scope S_0x5a70eadbb4f0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a70eae03f60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a70eae04000_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5a70eadbb4f0;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x5a70eae03f60_0;
    %inv;
    %store/vec4 v0x5a70eae03f60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5a70eadbb4f0;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "build/wave.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a70eadbb4f0 {0 0 0};
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a70eae04000_0, 0, 1;
    %delay 1000000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test/tb_top.v";
    "src/top.v";
